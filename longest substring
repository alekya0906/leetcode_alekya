class Solution:
    def lengthOfLongestSubstring(self, s: str) -> int:
        unl = set()
        left = 0
        ml = 0

        for right in range(len(s)):
            while s[right] in unl:
                unl.remove(s[left])
                left += 1

            unl.add(s[right])
            ml = max(ml, right - left + 1)
        
        return ml
